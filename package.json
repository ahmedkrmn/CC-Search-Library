    "node-fetch": "^2.3.0"
  },
  "devDependencies": {
    "docdash": "^1.0.3",
    "eslint": "^5.15.3",
    "jest": "^24.5.0",
    "jsdoc": "^3.5.5",
    "minami": "^1.2.3",
    "nock": "^10.0.6",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  }
